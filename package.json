{
	"name": "FootageViewer",
	"version": "1.0.0",
	"description": "Roller Derby Footage Tracker",
	"main": "app.js",
	"engines": {
		"node": ">=8"
	},
	"repository": "https://github.com/DC-1680/FootageViewer",
	"private": true,
	"cloud-repo-tools": {
		"requiresKeyFile": true,
		"requiresProjectId": true
	},
	"scripts": {
