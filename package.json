		"react-router-dom": "^4.3.1",
		"volleyball": "^1.5.1",
		"webpack": "^3.10.0",
		"webpack-cli": "^3.1.2"
	},
	"devDependencies": {
		"nodemon": "^1.14.11",
		"babel-core": "^6.26.0",
